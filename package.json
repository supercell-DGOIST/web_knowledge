{
  "name": "web-knowladge",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "lint-staged": {
    "*.{vue,js,ts}": [
      "npm run format"
    ],
    "*.{css,less}": [
      "npm run format"
    ]
  },
  "scripts": {
    "dev": "vite -c ./scripts/dev.ts --mode development",
    "build": "vue-tsc && vite build -c ./scripts/build.ts --mode production",
    "preview": "vite preview -c ./scripts/stats.ts --mode production",
    "format": "prettier --write src/",
    "lint": "eslint --fix ./src",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:3030 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3030 'cypress open --e2e'",
    "stats": "vue-tsc && vite build -c ./scripts/stats.ts --mode production",
    "prepare": "husky install",
    "commit": "cz"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "element-plus": "^2.3.8",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cypress": "^12.17.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.1",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.3",
    "prettier": "3.0.0",
    "rollup-plugin-gzip": "^3.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "start-server-and-test": "^2.0.0",
    "terser": "^5.19.0",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.0",
    "vitest": "^0.33.0",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  }
}
